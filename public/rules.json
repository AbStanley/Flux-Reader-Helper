[
    {
        "id": 1,
        "priority": 1,
        "action": {
            "type": "modifyHeaders",
            "requestHeaders": [
                {
                    "header": "Origin",
                    "operation": "remove"
                }
            ],
            "responseHeaders": [
                {
                    "header": "Access-Control-Allow-Origin",
                    "operation": "set",
                    "value": "*"
                },
                {
                    "header": "Access-Control-Allow-Methods",
                    "operation": "set",
                    "value": "*"
                },
                {
                    "header": "Access-Control-Allow-Headers",
                    "operation": "set",
                    "value": "*"
                },
                {
                    "header": "Access-Control-Allow-Private-Network",
                    "operation": "set",
                    "value": "true"
                }
            ]
        },
        "condition": {
            "regexFilter": "^http://.*:11434/.*",
            "resourceTypes": [
                "xmlhttprequest"
            ],
            "initiatorSchemes": [
                "chrome-extension"
            ]
        }
    }
]